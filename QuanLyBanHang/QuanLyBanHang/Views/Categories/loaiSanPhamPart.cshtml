@model IEnumerable<Table.Category>
<h2>Các loại sản phẩm</h2>
<hr />
@foreach (var item in Model)
{
    <div style="width:100%; height:50px; border:1px solid;">
        <img style="width:40px; height:40px; float:left;" src="~/Image/iconloai.png" />
        <b><a href="@Url.Action("laySanPhamTheoLoai","Categories",new {ma=item.CategoryID})">@item.CategoryName</a></b>
    </div>
}
